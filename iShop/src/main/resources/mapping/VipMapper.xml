    <select id="selectAllVipInfo" resultType="com.bizvane.ishop.entity.VIPInfo">
        SELECT * FROM
        DEF_VIP
        WHERE
        <if test="corp_code!=null and corp_code!=''">
            corp_code=#{corp_code}
        </if>
        <if test="search_value!=null and search_value!=''">
            and
            VIP_NAME like #{search_value}
            OR VIP_TYPE LIKE #{search_value}
            OR VIP_CARD_NUMBER LIKE #{search_value}
            OR SEX like #{search_value}
            OR USER_CODE LIKE #{search_value}
            OR STORE_CODE LIKE #{search_value}
            OR MODIFIER LIKE #{search_value}
            OR CREATER LIKE #{search_value}
            OR ISACTIVE LIKE #{search_value}
