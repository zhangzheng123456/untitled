<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.bizvane.ishop.dao.VipTagTypeMapper">

    <resultMap id="vipTagTypeInfo" type="com.bizvane.ishop.entity.VipTagType">
        <id column="id" property="id"/>
        <result column="type_code" property="type_code"/>
        <result column="type_name" property="type_name"/>
        <result column="type_description" property="type_description"/>
        <result column="modified_date" property="modified_date"/>
        <result column="modifier" property="modifier"/>
        <result column="creater" property="creater"/>
        <result column="created_date" property="created_date"/>
        <result column="isactive" property="isactive"/>
        <result column="corp_code" property="corp_code"/>
        <association property="corp">
            <result column="corp_code" property="corp_code"/>
        </association>
    </resultMap>

    <insert id="insert" parameterType="com.bizvane.ishop.entity.VipTagType">
        insert into def_vip_tag_type
        <trim prefix="(" suffixOverrides="," suffix=")">
            <if test="type_code!=null and type_code!=''">
                type_code,
            </if>
            <if test="type_name!=null  and type_name!=''">
                type_name ,
            </if>
            <if test="type_description!=null and type_description!=''">
                type_description ,
            </if>
            <if test="modified_date!=null and modified_date!=''">
                modified_date ,
            </if>
            <if test="modifier!=null and modifier!=''">
                modifier ,
            </if>
            <if test="creater!=null and creater!=''">
                creater,
            </if>
            <if test="created_date!=null and created_date!=''">
                created_date ,
            </if>
            <if test="isactive!=null and  isactive!=''">
                isactive ,
            </if>
            <if test="corp_code!=null and  corp_code!=''">
                corp_code ,
            </if>
        </trim>
        <trim prefix="values(" suffix=")" suffixOverrides=",">

            <if test="type_code!=null and type_code!=''">
                #{type_code},
            </if>
            <if test="type_name!=null  and type_name!=''">
                #{type_name},
            </if>
            <if test="type_description!=null and type_description!=''">
                #{type_description},
            </if>
            <if test="modified_date!=null and modified_date!=''">
                #{modified_date} ,
            </if>
            <if test="modifier!=null and modifier!=''">
                #{modifier} ,
            </if>
            <if test="creater!=null and creater!=''">
                #{creater},
            </if>
            <if test="created_date!=null and created_date!=''">
                #{created_date} ,
            </if>
            <if test="isactive!=null and  isactive!=''">
                #{isactive} ,
            </if>
            <if test="corp_code!=null and  corp_code!=''">
                #{corp_code} ,
            </if>
        </trim>
    </insert>

    <update id="updateByPrimaryKey" parameterType="com.bizvane.ishop.entity.VipTagType">
        update def_vip_tag_type
        <trim prefix="set" suffixOverrides=",">
            <if test="type_code!=null and type_code!=''">
                type_code=#{type_code},
            </if>
            <if test="type_name!=null  and type_name!=''">
                type_name=#{type_name},
            </if>

            <if test="type_description!=null and type_description!=''">
                type_description=#{type_description},
            </if>

            <if test="modified_date!=null and modified_date!=''">
                modified_date=#{modified_date} ,
            </if>

            <if test="modifier!=null and modifier!=''">
                modifier=#{modifier} ,
            </if>

            <if test="creater!=null and creater!=''">
                creater=#{creater},
            </if>

            <if test="created_date!=null and created_date!=''">
                created_date=#{created_date} ,
            </if>

            <if test="isactive!=null and  isactive!=''">
                isactive=#{isactive} ,
            </if>

            <if test="corp_code!=null and  corp_code!=''">
                corp_code=s#{corp_code} ,
            </if>
        </trim>
    </update>

    <delete id="deleteByPrimaryKey">
          delete FROM  def_vip_tag_type WHERE id=#{id}
    </delete>

    <select id="selectAllVipTagType" resultMap="vipTagTypeInfo">
        select
        vtt.id,
        vtt.type_code,
        vtt.type_name,
        vtt.type_type_description,
        vtt.modified_date,
        vtt.modifier,
        vtt.creater,
        vtt.created_date,
        vtt.isactive
        vtt.corp_code
        FROM def_vip_tag_type vtt
        WHERE
        1=1
        <if test="search_value!=null and search_value!=''">
            and vtt.type_name like concat('%',#{search_value},'%')
            or type_type_description like concat('%',#{search_value},'%')
            or modified_date like concat('%',#{search_value},'%')
            or modifier like concat('%',#{search_value},'%')
            or creater like concat('%',#{search_value},'%')
            or created_date like concat('%',#{search_value},'%')
            or isactive like concat('%',#{search_value},'%')
        </if>
        ORDER BY vtt.id
    </select>

    <select id="searchCode" resultMap="vipTagTypeInfo">
           select
            *
        FROM def_vip_tag_type vtt
        WHERE
        vtt.corp_code=#{corp_code}
        and
        vtt.type_code=#{type_code}
    </select>

    <select id="selectName" resultType="com.bizvane.ishop.entity.VipTagType">
        select
            *
        FROM def_vip_tag_type vtt
        WHERE
        vtt.corp_code=#{corp_code}
        and
        vtt.type_name=#{type_name}
    </select>


