<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.bizvane.ishop.dao.LoginLogMapper">

    <select id="selectByLogId" resultType="com.bizvane.ishop.entity.LoginLog">
        SELECT *
        FROM acc_login_log
        WHERE 
        <if test="log_id != 0">
            ID = #{log_id}
        </if>
        <if test="phone != null and phone !=''">
            phone = #{phone}
        </if>
    </select>

    <insert id="insertLoginLog" parameterType="com.bizvane.ishop.entity.LoginLog">
        INSERT INTO acc_login_log
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="phone!=null">
                phone,
            </if>
            <if test="content!=null">
                content,
            </if>
            <if test="platform!=null">
                platform,
            </if>
            <if test="modified_date != null">
                modified_date,
            </if>
            <if test="modifier != null">
                modifier,
            </if>
            <if test="created_date != null">
                created_date,
            </if>
            <if test="creater != null">
                creater,
            </if>
            <if test="isactive != null">
                isactive,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="phone!=null">
                #{phone},
            </if>
            <if test="content!=null">
                #{content},
            </if>
            <if test="platform!=null">
                #{platform},
            </if>
            <if test="modified_date != null">
                #{modified_date},
            </if>
            <if test="modifier != null">
                #{modifier},
            </if>
            <if test="created_date != null">
                #{created_date},
            </if>
            <if test="creater != null">
                #{creater},
            </if>
            <if test="isactive != null">
                #{isactive},
            </if>
        </trim>
    </insert>

    <update id="updateByLogId" parameterType="com.bizvane.ishop.entity.LoginLog">
        UPDATE acc_login_log
        <trim prefix="SET" suffixOverrides=",">
            <if test="phone!=null and phone!='' ">
                phone = #{phone},
            </if>
            <if test="content!=null and content!='' ">
                content = #{content},
            </if>
            <if test="platform!=null and platform!='' ">
                platform = #{platform},
            </if>
            <if test="modified_date!=null">
                modified_date = #{modified_date},
            </if>
            <if test="modifier!=null and modifier!='' ">
                modifier = #{modifier},
            </if>
            <if test="created_date!=null">
                created_date = #{created_date},
            </if>
            <if test="creater!=null and creater!='' ">
                creater = #{creater},
            </if>
            <if test="isactive!=null and isactive!='' ">
                isactive = #{isactive},
            </if>
        </trim>
        WHERE ID = #{id};
    </update>

    <delete id="deleteByLogId">
        DELETE FROM
        acc_login_log
        WHERE ID = #{id}
    </delete>

</mapper>